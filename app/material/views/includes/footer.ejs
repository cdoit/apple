    function show_add(title,url,w,h){
        layer_show(title,url,w,h);
    }

    function member_show(title,url,id,w,h){
	        layer_show(title,url,w,h);
        }
    /*-添加*/
    function equipment_add(title,url,w,h){
        var index = layer.open({
            type: 2,
            title: title,
            content: url
        });
        layer.full(index);
    }
    /*-编辑*/
    function equipment_edit(title,url,id,w,h){
        var index = layer.open({
            type: 2,
            title: title,
            content: url
        });
        layer.full(index);
    }
    /*-删除*/
    function list_del(obj,url){
        layer.confirm('确认要删除吗？',function(index){
            $.ajax({
                type: 'GET',
                url: url,
                dataType: 'json',
                success: function(data){
                    $(obj).parents("tr").remove();
                    layer.msg('已删除!',{icon:1,time:1000});
                },
                error:function(data) {
                    console.log(data.msg);
                },
            });		
        });
    }

    function nextPage(pageNo,totalPage){
        pageNo = parseInt(pageNo);
        totalPage = parseInt(totalPage);
        if(pageNo >= totalPage){
            return false;
        }
        return true;
    }

    function prevPage(pageNo){
        if(pageNo == '1'){
            return false;
        }
        return true;
    }
</script>
