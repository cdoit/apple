<!--_meta 作为公共模版分离出去-->
<!DOCTYPE HTML>
<html>

<head>
    <% include ../includes/meta.ejs %>
    <!--/meta 作为公共模版分离出去-->
    <title>悉道设备云监控</title>
</head>

<body>
            <nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 首页
                <span class="c-gray en">&gt;</span>
                当前设计的任务列表
                <a class="btn btn-success radius r" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a>
            </nav>
            <div class="Hui-article">
                <article class="cl pd-20">
                   
                    <div class="mt-20">
                            <table class="table table-border table-bordered table-bg table-sort">
                                    <thead>
                                        <tr class="text-c">
                                            <!-- <th width="25"><input type="checkbox" name="" value=""></th> -->
                                            <th width="120">任务名称</th>
                                            <th width="120">任务地址</th>
                                            <th width="120">方案详情</th>
                                            <%if(flag!=null){%>
                                            <th width="120">设计详情</th>
                                            <% } %>  
                                            <!-- <th width="120">生产设备详情</th> -->
                                            <!-- <th width="120">  接单人   </th> -->
                                            <th width="120">审核状态</th>
                                            <th width="120">任务进度</th>
                                            <th width="100">操作</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    <%if(project!=null&&project.length>0){
                                        <!-- <% -->
                                         for(var i=0; i<project.length; ++i) { %>  
                                        <tr class="text-c">
                                            <!-- <td><input name="" type="checkbox" value=""></td> -->
                                            <td><%=project[i].name %> </td>
                                            <td><%=project[i].address %> </td>
                                            <td>
                                                <%if(project[i].schemeId!=null){%>
                                                    <u style="cursor:pointer" class="text-primary" onclick="member_show('查看方案信息','/scheme/findById?schemeId=<%=project[i].schemeId%>','10001','360','400')">查看</u>
                                                <%}%>
                                                <%if(project[i].schemeId==null){%>
                                                    暂未绑定方案数据
                                                <%}%>
                                            </td>


                                            <%if(flag!=null){%>
                                            <td>
                                                <%if(project[i].designId!=null){%>
                                                    <u style="cursor:pointer" class="text-primary" onclick="member_show('查看设计信息','/design/findById?designId=<%=project[i].designId%>','10001','360','400')">查看</u>
                                                <%}%>
                                                <%if(project[i].designId==null){%>
                                                    暂未绑定设计数据 <u style="cursor:pointer" class="text-primary" onclick="member_show('上传设计信息','/design/add?projectId=<%=project[i].id%>','10001','360','400')">上传？</u>
                                                <%}%>
                                            </td>
                                            <% } %>  

                                            <!-- <td>
                                                <%if(project[i].equipmentId!=null){%>
                                                    <u style="cursor:pointer" class="text-primary" onclick="member_show('查看设备信息','/equipment/findById?equipmentId=<%=project[i].equipmentId%>','10001','360','400')">查看</u>
                                                <%}%>
                                                <%if(project[i].equipmentId==null){%>
                                                    暂未绑定设备数据
                                                <%}%>
                                            </td> -->
                                            <!-- <td>
                                                <%if(project[i].designerId!=null){%>
                                                    <u style="cursor:pointer" class="text-primary" onclick="member_show('查看接单人信息','/design/findById?designId=<%=project[i].designerId%>','10001','360','400')">查看</u>
                                                <%}%>
                                                <%if(project[i].designerId==null){%>
                                                    暂未接单
                                                <%}%>
                                            </td> -->
                                            <td><%=project[i].auditstate %> </td>
                                            <td><%=project[i].progress %> </td>
                                            <td class="f-14 product-brand-manage">
                                                <!-- <a style="text-decoration:none" class="ml-5"  _href="/project/fenfaMap" onclick="project_fenfa('地图页面分发','/project/fenfaMap')" href="javascript:;" title="地图页面分发"><i class="Hui-iconfont">&#xe665;</i></a> -->
                                                <%if(project[i].designerId==null){%>
                                                    <a style="text-decoration:none" class="ml-5" onClick="list_jiedan('0','/project/jiedan?projectId=<%=project[i].id %>')" href="javascript:;" title="接单"><i class="Hui-iconfont">&#xe6de;</i></a>
                                                <%}%>
                                                <%if(project[i].designerId!=null){%>
                                                    <a style="text-decoration:none" class="ml-5" onClick="list_jiedan('1','/project/jiedan?flag=1&projectId=<%=project[i].id %>')" href="javascript:;" title="取消接单"><i class="Hui-iconfont">&#xe6e2;</i></a>
                                                <%}%>
                                            </td>
                                        </tr>
                                        <% }} %>  
                                    </tbody>
                                </table>
                    </div>
                </article>
            </div>
            
        <% include ../includes/footer.ejs %>
        <script type="text/javascript" >
        function list_jiedan(obj,url){
        var msg ;
        if(obj == '1'){
            msg = "确认要取消接单吗？";
        }else if(obj == '0'){
            msg = "确认要接单吗？";
        }
        layer.confirm(msg,function(index){
            window.location.href = url;	
        });
    }
        </script>
   
</body>

</html>