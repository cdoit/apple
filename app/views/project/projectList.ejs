<!--_meta 作为公共模版分离出去-->
<!DOCTYPE HTML>
<html>

<head>
    <% include ../includes/meta.ejs %>
    <!--/meta 作为公共模版分离出去-->
    <title>悉道设备云监控</title>
</head>

<body>
            <nav class="breadcrumb"><i class="Hui-iconfont">
                <a class="btn btn-success radius r" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a>
            </nav>
            <div class="Hui-article">
                <article class="cl pd-20">
                    <form class="form form-horizontal" method="post" action="/project/projectList" >
                        <div class="text-c">
                            <input type="text" name="keyword" id="" value="<%if(keyword!=null){%><%=keyword%><%}%>" placeholder=" 任务名称" style="width:250px" class="input-text">
                            <button name="" id="" class="btn btn-success" type="submit"><i class="Hui-iconfont">&#xe665;</i> 搜任务</button>
                        </div>
                    </form>
                    <div class="cl pd-5 bg-1 bk-gray mt-20">
                        <span class="l">
                        <!-- <a href="javascript:;" onclick="datadel()" class="btn btn-danger radius"><i class="Hui-iconfont">&#xe6e2;</i> 批量删除</a> -->
                        <a class="btn btn-primary radius" data-title="添加任务" _href="/project/add" onclick="show_add('添加任务','/project/add')" href="javascript:;"><i class="Hui-iconfont">&#xe600;</i> 添加任务</a>
                        </span>
                        <span class="r">共有数据：<strong>***</strong> 条</span>
                    </div>
                    <div class="mt-20">
                            <table class="table table-border table-bordered table-bg table-sort">
                                    <thead>
                                        <tr class="text-c">
                                            <!-- <th width="25"><input type="checkbox" name="" value=""></th> -->
                                            <th width="120">任务名称</th>
                                            <th width="120">任务地址</th>
                                            <th width="120">方案详情</th>
                                            <!-- <th width="120">设计详情</th>
                                            <th width="120">生产设备详情</th> -->
                                            <th width="120">审核状态</th>
                                            <th width="120">任务进度</th>
                                            <th width="100">操作</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% for(var i=0; i<project.length; ++i) { %>  
                                        <tr class="text-c">
                                            <!-- <td><input name="" type="checkbox" value=""></td> -->
                                            <td><%=project[i].name %> </td>
                                            <td><%=project[i].address %> </td>
                                            <td>
                                                <%if(project[i].schemeId!=null){%>
                                                    <u style="cursor:pointer" class="text-primary" onclick="member_show('查看方案信息','/scheme/findById?schemeId=<%=project[i].schemeId%>','10001','360','400')">查看</u>
                                                <%}%>
                                            </td>
<!-- 
                                            <td>
                                                <%if(project[i].designId!=null){%>
                                                    <u style="cursor:pointer" class="text-primary" onclick="member_show('查看设计信息','/design/findById?designId=<%=project[i].designId%>','10001','360','400')">查看</u>
                                                <%}%>
                                                <%if(project[i].designId==null){%>
                                                    暂未绑定设计数据
                                                <%}%>
                                            </td>
                                            <td>
                                                <%if(project[i].equipmentId!=null){%>
                                                    <u style="cursor:pointer" class="text-primary" onclick="member_show('查看设备信息','/equipment/findById?equipmentId=<%=project[i].equipmentId%>','10001','360','400')">查看</u>
                                                <%}%>
                                                <%if(project[i].equipmentId==null){%>
                                                    暂未绑定设备数据
                                                <%}%>
                                            </td>
                                             -->
                                            <td><%=project[i].auditstate %> </td>
                                            <td><%=project[i].progress %> </td>
                                            <td class="f-14 product-brand-manage">
                                                <a style="text-decoration:none" onClick="show_add('编辑任务','/project/add?projectId=<%=project[i].id %>')" href="javascript:;" title="编辑"><i class="Hui-iconfont">&#xe6df;</i></a> 
                                                <a style="text-decoration:none" class="ml-5" onClick="list_del(this,'/project/delete?projectId=<%=project[i].id %>')" href="javascript:;" title="删除"><i class="Hui-iconfont">&#xe6e2;</i></a>
                                            </td>
                                        </tr>
                                        <% } %>  
                                    </tbody>
                                </table>
                    </div>
                </article>
            </div>
            
        <% include ../includes/footer.ejs %>
        <script type="text/javascript" >
        
        function member_show(title,url,id,w,h){
	        layer_show(title,url,w,h);
        }

        function project_fenfa(title,url,w,h){
            var index = layer.open({
                type: 2,
                title: title,
                content: url
            });
            layer.full(index);
        }

        function project_fenfaList(title,url,w,h){
            var index = layer.open({
                type: 2,
                title: title,
                content: url
            });
            layer.full(index);
        }

        </script>
   
</body>

</html>