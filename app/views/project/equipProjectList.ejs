<!--_meta 作为公共模版分离出去-->
<!DOCTYPE HTML>
<html>

<head>
    <% include ../includes/meta.ejs %>
    <!--/meta 作为公共模版分离出去-->
    <title>悉道设备云监控</title>
</head>

<body>
            <nav class="breadcrumb"><i class="Hui-iconfont">&#xe67f;</i> 首页
                <span class="c-gray en">&gt;</span>
                当前机器的任务列表
                <a class="btn btn-success radius r" style="line-height:1.6em;margin-top:3px" href="javascript:location.replace(location.href);" title="刷新" ><i class="Hui-iconfont">&#xe68f;</i></a>
            </nav>
            <div class="Hui-article">
                <article class="cl pd-20">
                    <form action="/project/findByEquiCode" method="post" role="form">
                        <div class="text-c">
                            <input type="text" name="equipmentCode" value="<%=equipmentCode %>" id="" placeholder="机器编码" style="width:250px" class="input-text">
                            <button name="" id="" class="btn btn-success" type="submit"><i class="Hui-iconfont">&#xe665;</i> 搜任务</button>
                        </div>
                    </form>
                    <!-- <div class="cl pd-5 bg-1 bk-gray mt-20">
                        <span class="l">
                        <a href="javascript:;" onclick="datadel()" class="btn btn-danger radius"><i class="Hui-iconfont">&#xe6e2;</i> 批量删除</a>
                        </span>
                        <span class="r">共有数据：<strong>***</strong> 条</span>
                    </div> -->
                    <div class="mt-20">
                            <table class="table table-border table-bordered table-bg table-sort">
                                    <thead>
                                        <tr class="text-c">
                                            <!-- <th width="25"><input type="checkbox" name="" value=""></th> -->
                                            <th width="120">任务名称</th>
                                            <th width="120">任务地址</th>
                                            <th width="120">方案详情</th>
                                            <th width="120">设计详情</th>
                                            <th width="120">生产设备详情</th>
                                            <th width="120">审核状态</th>
                                            <th width="120">任务进度</th>
                                            <th width="100">操作</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                    <%if(project!=null&&project.length>0){
                                        <!-- <% -->
                                         for(var i=0; i<project.length; ++i) { %>  
                                        <tr class="text-c">
                                            <!-- <td><input name="" type="checkbox" value=""></td> -->
                                            <td><%=project[i].name %> </td>
                                            <td><%=project[i].address %> </td>
                                            <td>
                                                <%if(project[i].scheme_id!=null){%>
                                                    <u style="cursor:pointer" class="text-primary" onclick="member_show('查看方案信息','/scheme/findById?schemeId=<%=project[i].scheme_id%>','10001','360','400')">查看</u>
                                                <%}%>
                                                <%if(project[i].scheme_id==null){%>
                                                    暂未绑定方案数据
                                                <%}%>
                                            </td>
                                            <td>
                                                <%if(project[i].design_id!=null){%>
                                                    <u style="cursor:pointer" class="text-primary" onclick="member_show('查看设计信息','/design/findById?designId=<%=project[i].design_id%>','10001','360','400')">查看</u>
                                                <%}%>
                                                <%if(project[i].design_id==null){%>
                                                    暂未绑定设计数据
                                                <%}%>
                                            </td>
                                            <td>
                                                <%if(project[i].equipment_id!=null){%>
                                                    <u style="cursor:pointer" class="text-primary" onclick="member_show('查看设备信息','/equipment/findById?equipmentId=<%=project[i].equipment_id%>','10001','360','400')">查看</u>
                                                <%}%>
                                                <%if(project[i].equipment_id==null){%>
                                                    暂未绑定设备数据
                                                <%}%>
                                            </td>
                                            <td><%=project[i].auditstate %> </td>
                                            <td><%=project[i].progress %> </td>
                                            <td class="f-14 product-brand-manage">
                                                <!-- <a style="text-decoration:none" class="ml-5"  _href="/project/fenfaMap" onclick="project_fenfa('地图页面分发','/project/fenfaMap')" href="javascript:;" title="地图页面分发"><i class="Hui-iconfont">&#xe665;</i></a> -->
                                                <%if(project[i].designId!=null){%>
                                                <a style="text-decoration:none" _href="/project/fenfaList" onclick="project_fenfaList('龙骨生产设备分发','/project/fenfaList?projectId=<%=project[i].id %>')" href="javascript:;" title="列表页面分发"><i class="Hui-iconfont">&#xe615;</i></a>
                                                <%}%>
                                            </td>
                                        </tr>
                                        <% }} %>  
                                    </tbody>
                                </table>
                    </div>
                </article>
            </div>
            
        <% include ../includes/footer.ejs %>
        <script type="text/javascript" >
        
        function member_show(title,url,id,w,h){
	        layer_show(title,url,w,h);
        }

        function project_fenfa(title,url,w,h){
            var index = layer.open({
                type: 2,
                title: title,
                content: url
            });
            layer.full(index);
        }

        function project_fenfaList(title,url,w,h){
            var index = layer.open({
                type: 2,
                title: title,
                content: url
            });
            layer.full(index);
        }

        </script>
   
</body>

</html>